{
  "rootDir": "..",
  "roots": [
    "<rootDir>/src"
  ],
  "moduleFileExtensions": [ "ts", "tsx", "js", "jsx" ],
  "transform": {
    "^.+\\.(ts|tsx)$": "<rootDir>/.jest/preprocessor.js"
  },
  "testRegex": "\\.test\\.(?:ts|tsx)$",
  "transformIgnorePatterns": [
    "/node_modules/"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "globals": {
    "DEVELOPMENT": false,
    "FAKE_SERVER": false
  }
}
